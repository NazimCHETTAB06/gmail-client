{
  "buildCommand": "echo 'Static frontend'",
  "installCommand": "echo 'No dependencies'",
  "framework": "static",
  "public": "frontend",
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}
